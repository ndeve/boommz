<div class="actions actions-sex">
    <div class="button is-light sex m" data-src="/persona/creator/men/ID.png"
         title="{% trans %}Homme{% endtrans %}">
        {% include "/svg/male.html.twig" %}
    </div>
    <div class="button is-light sex w on" data-src="/persona/creator/women/ID.png"
         title="{% trans %}Femme{% endtrans %}">
        {% include "/svg/female.html.twig" %}
    </div>
</div>
<div class="actions actions-main" data-id="main">
    <div class="button is-light color on" title="{% trans %}Choisis ta couleur{% endtrans %}" data-color="0">
        <div class="color-0"></div>
    </div>
    <div class="button is-light plus" title="{% trans %}Choisis ta gueule{% endtrans %}" data-sect="head">
        <img src="/persona/creator/head2.png"/>
    </div>
    <div class="button is-light clothes plus" title="{% trans %}Choisis tes habits{% endtrans %}" data-sect="clothes">
        <img src="/persona/creator/vest.png"/>
    </div>
    <div class="button is-light dice">
        {% include "/svg/dice.html.twig" %}
    </div>
</div>

<div class="actions actions-main head is-hidden">
    <div class="button is-light back">
        {% include "/svg/back.html.twig" %}
    </div>
    <div class="button is-light sub on" title="{% trans %}Choisis tes cheveux{% endtrans %}"
         data-type="hair"
         data-max="36"
         data-sect="head"
         data-src="/persona/creator/hair/ID.png">
        <img src="/persona/creator/head.png"/>
        <img src="/persona/creator/hair/0026.png"/>
    </div>
    <div class="button is-light sub" title="{% trans %}Choisis tes yeux{% endtrans %}"
         data-type="eyes"
         data-max="29"
         data-sect="head"
         data-src="/persona/creator/eyes/ID.png">
        <img src="/persona/creator/head.png"/>
        <img src="/persona/creator/eyes/0001.png"/>
    </div>
    <div class="button is-light sub" title="{% trans %}Choisis ton nez{% endtrans %}"
         data-type="nose"
         data-max="8"
         data-sect="head"
         data-src="/persona/creator/nose/ID.png">
        <img src="/persona/creator/head.png"/>
        <img src="/persona/creator/nose/0001.png"/>
    </div>
    <div class="button is-light sub" title="{% trans %}Choisis ta bouche{% endtrans %}"
